{
    "name": "puntodev/bookables",
    "description": "Bookable Library",
    "keywords": [
        "puntodev",
        "bookables"
    ],
    "homepage": "https://github.com/puntodev/bookables",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Mariano Goldman",
            "email": "mariano.goldman@puntodev.com.ar",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "league/period": "^5",
        "nesbot/carbon": "^2.54"
    },
    "require-dev": {
        "mockery/mockery": "^1.5",
        "phpunit/phpunit": "^10",
        "nunomaduro/collision": "^7"
    },
    "autoload": {
        "psr-4": {
            "Puntodev\\Bookables\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
